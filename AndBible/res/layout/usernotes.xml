<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/usernotes" android:layout_height="wrap_content" android:text="@string/usernotes" android:layout_width="match_parent"></TextView>
    <ListView android:id="@android:id/list" android:layout_width="fill_parent" android:visibility="visible" android:choiceMode="singleChoice" android:layout_height="424dp"></ListView>
</LinearLayout>